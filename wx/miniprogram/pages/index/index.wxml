<map
 id="map"
 latitude="{{location.latitude}}"
 longitude="{{location.longitude}}"
 scale="{{scale}}"
 setting="{{setting}}"
 enable-overlooking="{{isOverLooking}}"
 enable-3D="{{is3D}}"
 markers="{{markers}}"
>
	<view class="icon-container">
		<image class="round" src="{{avatarURL?avatarURL:'/resources/account.png'}}" />
		<image bindtap="onMyLocationTap" src="/resources/my_location.png" />
	</view>
	<view class="btn-container bg-grey round" bindtap="onScanClicked">
		<view>扫码租车</view>
	</view>
</map>


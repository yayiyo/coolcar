page {
    width: 100%;
    height: 100%;
}

#map {
    width: 100%;
    height: 100%;
}

.icon-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    width: 120rpx;
    height: 190rpx;
    margin-top: 100rpx;
}

.icon-container image {
    width: 80rpx;
    height: 80rpx;
}

.btn-container {
    width: 600rpx;
    position: absolute;
    bottom: 8%;
    margin-left: 75rpx;
    height: 62rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    opacity: 0.9;
}
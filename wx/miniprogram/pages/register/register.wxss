.lic-tmpl {
    width: 600rpx;
    height: 450rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.stretch {
    width: 100%;
    height: 100%;
}

.lic-tmpl-img {
    position: absolute;
    opacity: 0.2;
}

.instruction {
    position: absolute;
    bottom: -74rpx;
}

.cuIcon-camerafill {
    font-size: 172rpx;
}

.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}

.lic-form {
    width: 600rpx;
}

.check {
    position: absolute;
    top: 0;
    right: 0;
    width: 100rpx;
    height: 100rpx;
}